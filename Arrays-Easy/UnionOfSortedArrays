vector<int> findUnion(int arr1[], int arr2[], int n, int m)
    {
      set<int> set1;
      
        
        for(int i=0;i<n;i++){
            set1.insert(arr1[i]);
        }
        for(int i=0;i<m;i++){
            set1.insert(arr2[i]);
        }
       vector<int> vec(set1.begin(), set1.end());

    return vec;}
